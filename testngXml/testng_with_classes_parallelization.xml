<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite with parallelization" parallel="classes" thread-count="2">
    <test name="Tests parallelization">
        <classes>
            <class name="FirstTestClass">
                <methods>
                    <exclude name="fourthTest"/>
                </methods>
            </class>
            <class name="SecondTestClass">
                <methods>
                    <exclude name="fourthTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>